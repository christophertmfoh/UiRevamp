{
  "name": "fablecraft-enterprise-pro",
  "version": "2.0.0",
  "description": "Enterprise-grade creative writing platform for professionals",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "build": "tsc && vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview --host 0.0.0.0 --port 4173",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "clean": "rm -rf dist coverage .nyc_output node_modules/.vite",
    "clean:all": "npm run clean && rm -rf node_modules",
    "analyze": "npx vite-bundle-analyzer",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@tanstack/react-router": "^1.130.8",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "zustand": "^5.0.6",
    "immer": "^10.0.4",
    "react-hook-form": "^7.61.1",
    "@hookform/resolvers": "^5.2.1",
    "zod": "^4.0.13",
    "axios": "^1.11.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-menubar": "^1.1.2",
    "@radix-ui/react-context-menu": "^2.2.2",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-sheet": "^0.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1",
    "lucide-react": "^0.534.0",
    "cmdk": "^1.0.0",
    "sonner": "^1.7.1",
    "react-day-picker": "^9.3.2",
    "date-fns": "^4.1.0",
    "react-resizable-panels": "^2.1.6",
    "react-hotkeys-hook": "^4.6.1",
    "embla-carousel-react": "^8.5.1",
    "vaul": "^1.1.1",
    "react-dropzone": "^14.3.5",
    "@tiptap/react": "^2.12.0",
    "@tiptap/starter-kit": "^2.12.0",
    "@tiptap/extension-placeholder": "^2.12.0",
    "@tiptap/extension-character-count": "^2.12.0",
    "@tiptap/extension-collaboration": "^2.12.0",
    "@tiptap/extension-collaboration-cursor": "^2.12.0",
    "y-websocket": "^2.0.4",
    "yjs": "^13.6.20",
    "framer-motion": "^12.1.2",
    "react-virtual": "^2.10.5",
    "react-window": "^1.8.10",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.0.3",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.9"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/node": "^24.1.0",
    "@types/uuid": "^11.0.1",
    "@types/lodash-es": "^4.17.15",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react-swc": "^4.2.1",
    "vite": "^7.0.4",
    "typescript": "^5.8.3",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "postcss": "^8.5.6",
    "autoprefixer": "^10.4.21",
    "tailwindcss-animate": "^1.0.7",
    "vitest": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "jsdom": "^26.0.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "playwright": "^1.49.1",
    "@playwright/test": "^1.49.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite-bundle-analyzer": "^0.12.1",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-a11y": "^8.4.7",
    "msw": "^2.6.8",
    "msw-storybook-addon": "^2.0.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}