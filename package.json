{
  "name": "rest-express",
  "version": "3.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently --kill-others \"npm:server:dev\" \"npm:client:dev\"",
    "server:dev": "NODE_ENV=development tsx server/index.ts",
    "client:dev": "vite --config client/vite.config.ts",
    "build": "vite build --config client/vite.config.ts",
    "start": "NODE_ENV=production node server/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run lint:fix && npm run format && npm run type-check",
    "validate-env": "node scripts/validate-env.js",
    "generate-secrets": "node scripts/generate-secrets.cjs",
    "security-scan": "npm audit --audit-level=moderate && node scripts/security-check.js",
    "health-check": "node scripts/health-check.js",
    "setup:dev": "npm run generate-secrets && cp .env.example .env.development && npm run validate-env",
    "dev:fast": "FORCE_OPTIMIZE=true concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "dev:performance": "NODE_ENV=development npm run dev",
    "dev:clean": "rm -rf dist node_modules/.vite && npm run dev",
    "build:analyze": "npm run build && echo 'ðŸ“Š Build complete! Check dist/ folder for output files.'",
    "replit:setup": "npm install && npm run generate-secrets && npm run validate-env",
    "replit:health": "curl -s http://localhost:5000/api/health | jq .",
    "replit:backup": "node -e \"console.log('Use the backup feature in the performance dashboard!')\"",
    "replit:clean": "rm -rf dist .vite node_modules/.cache && echo 'Cleaned build artifacts'",
    "replit:optimize": "npm run replit:clean && npm install && npm run dev:fast"
  },
