modules = ["nodejs-20", "web", "postgresql-16"]
run = "cd fablecraft-enterprise && npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable_24_05"

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5173
externalPort = 5173

[[ports]]
localPort = 5174
externalPort = 3000

[[ports]]
localPort = 24678
externalPort = 3001

[env]
PORT = "5000"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd fablecraft-enterprise && npm run dev"
waitForPort = 5174

[agent]
integrations = ["javascript_openai==1.0.0", "javascript_database==1.0.0", "javascript_anthropic==1.0.0", "javascript_log_in_with_replit==1.0.0", "javascript_gemini==1.0.0"]
